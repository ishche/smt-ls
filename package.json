{
    "name": "smt-ls",
    "displayName": "SMT Language Support",
    "description": "SMT Language Support vscode extensions",
    "version": "0.0.1",
    "author": {
        "name": "Iurii Shchekochikhin",
        "email": "amagex@gmail.com"
    },
    "repository": {
        "url": "https://github.com/ishche/smt-ls",
        "type": "git"
    },
    "homepage": "https://github.com/ishche/smt-ls",
    "engines": {
        "vscode": "^1.69.0"
    },
    "categories": [
        "Programming Languages", "Snippets"
    ],
    "contributes": {
        "languages": [{
            "id": "smt20",
            "extensions": ["z3"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "smt20",
            "scopeName": "source.smt20",
            "path": "./syntaxes/smt20.tmLanguage.json"
        }],
        "snippets": [{
            "language": "smt20",
            "path": "./snippets/smt20.snippets.json"
        }]
    }
}